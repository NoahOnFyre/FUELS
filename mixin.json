{
  "name": "Mixin",
  "version": "1.0.0",
  "author": "NoahOnFyre",
  "description": "Makes an injection in FyUTILS to change the main color.",
  "format": 1,
  "properties": {
      "type": "DEFAULT",
      "command_name": "inject"
  },
  "head": {
      "enabled": true,
      "status": "Injecting color...",
      "argument_length": 0,
      "arguments": []
  },
  "body": {
      "enabled": true,
      "content": [
          "print(prefix('INFO') + 'Setting color to red...')",
          "src_color = Fore.RED"
      ]
  }
}